{
    "plugins": {
        "@textlint/markdown": {
            "extensions": [
                ".md"
            ]
        }
    },
    "rules": {
        "common-misspellings": true,
        "preset-japanese": true,
        "no-todo": true,
        "preset-ja-technical-writing": false,
        "preset-jtf-style": true
    }
}